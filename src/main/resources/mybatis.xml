<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="defaultStatementTimeout" value="25" />
    </settings>
    <plugins>
        <plugin interceptor="com.kingsoft.lcgl.core.mybatis.PageInterceptor">
            <property name="dialectClass" value="com.kingsoft.lcgl.core.mybatis.dialect.MySQLDialect"/>
        </plugin>
    </plugins>
</configuration>